<% if(theme.head.rss && config.feed && config.feed.type) { %>
  <% if(config.feed.type == "atom"){ %>
    <link rel="alternate" type="application/atom+xml" href="<%= theme.head.rss || '#' %>">
  <% } else if(config.feed.type == "rss2") { %>
    <link rel="alternate" type="application/rss+xml" href="<%= theme.head.rss || '#' %>">
  <% }%>
<% } %>
