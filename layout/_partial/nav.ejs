<% if (theme.header.nav) { %>
<nav class="nav">
  <a id="nav-toggle" class="nav-toggle"><div></div></a>
  <div class="nav-wrapper">
    <ul>
      <% _.forIn(theme.header.nav, function(link, name){ %>
        <li><a class="nav-link <%- is_current(link) ? 'nav-link__active' : '' %>" href="<%= link %>"><%= name %></a></li>
      <% }) %>
    </ul>
  </div>
</nav>
<% } %>
